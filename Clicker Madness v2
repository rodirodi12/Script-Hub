--https://www.roblox.com/games/5490351219/FREE-PET-Clicker-Madness

local Library =
    loadstring(game:HttpGet("https://raw.githubusercontent.com/preztel/AzureLibrary/master/uilib.lua", true))()


local Auto_Farm = Library:CreateTab("Auto Farm", "", true)

Auto_Farm:CreateToggle("Otomatik Beast Mode", function(arg)
    if arg then
        _G.Condition = true -- true turns it on, false turns it off
        while _G.Condition == true do
        local A_1 = 1
        local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.BeastModeService.Begin
        Event:FireServer(A_1)
        wait(1)
    end
    else
        _G.Condition = false -- true turns it on, false turns it off
        while _G.Condition == true do
        local A_1 = 1
        local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.BeastModeService.Begin
        Event:FireServer(A_1)
        wait(1)
    end
    end
end)

Auto_Farm:CreateToggle("Otomatik Tıklama", function(state)
    if state then
        _G.Condition = true -- true turns it on, false turns it off
        while _G.Condition == true do
        local A_1 = 1
        local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.ClickService.Click
        Event:FireServer(A_1)
        wait()
    end
    else
        _G.Condition = false -- true turns it on, false turns it off
        while _G.Condition == true do
        local A_1 = 1
        local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.ClickService.Click
        Event:FireServer(A_1)
        wait()
    end
    end
end)

Auto_Farm:CreateToggle("TP Gems/Clicks", function(arg)
    if arg then
        _G.toggled = true
        while _G.toggled == true do
            wait(0)
            game.Workspace.ScriptObjects.BasePickup.HumanoidRootPart.CFrame =
                game.Workspace[game.Players.LocalPlayer.Name].HumanoidRootPart.CFrame
        end
    else
        _G.toggled = false
    end
end)

Auto_Farm:CreateToggle("Otomatik Boss", function(arg)
    if arg then
        _G.toggled = true
        while _G.toggled == true do
            wait(0)
            game.Workspace[game.Players.LocalPlayer.Name].HumanoidRootPart.CFrame =
                game.Workspace.ScriptObjects["Karen Keyboard"].HumanoidRootPart.CFrame * CFrame.new(0, 0, 5)
            game.ReplicatedStorage.Aero.AeroRemoteServices.CursorCannonService.FireBoss:FireServer("Karen Keyboard")
        end
    else
        _G.toggled = false
    end
end)

Auto_Farm:CreateToggle("Otomatik Kill All", function(arg)
    if arg then
        _G.toggled = true
        while _G.toggled == true do
            wait(0)

            for i, v in pairs(game.Players:GetChildren()) do
                if v.Name ~= game:GetService("Players").LocalPlayer.Name then
                    local A_1 = game:GetService("Players")[v.Name]
                    local Event =
                        game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.CursorCannonService.FireCursor
                    Event:FireServer(A_1)
                end
            end
        end
    else
        _G.toggled = false
    end
end)

local Auto_Normal_Upgrade = Library:CreateTab("Auto Normal Upgrade", "", true)

Auto_Normal_Upgrade:CreateToggle("Tıklama Çarpanı", function(arg)
    if arg then
        _G.Condition = true -- true turns it on, false turns it off
        while _G.Condition == true do
local A_1 = "ClickMultiply"
local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.UpgradeService.BuyUpgrade
Event:FireServer(A_1)    
        wait()
    end
    else
        _G.Condition = false -- true turns it on, false turns it off
        while _G.Condition == true do
            local A_1 = "ClickMultiply"
            local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.UpgradeService.BuyUpgrade
            Event:FireServer(A_1)   
        wait()
    end
    end
end)

