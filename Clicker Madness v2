local Teleport = Library:CreateTab("Teleport", "", true)

Teleport:CreateButton("Başlangıç", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(552, 9, -510)
end)

Teleport:CreateButton("Lav", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1776, 7, -737)
end)

Teleport:CreateButton("Çöl", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2215, 7, 1088)
end)

Teleport:CreateButton("Okyanus", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-258, 9, 2141)
end)

Teleport:CreateButton("Kış", function()
